@model IEnumerable<TIDIP_ADO_NET_V2.Models.Members>

@{
    ViewBag.Title = "Index";
}
<div class="mt-2">
    <h2><strong>我的會員</strong></h2>

</div>
@*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
<table class="table">
    <tr>
        <th>
            姓名
        </th>
        <th>
           電話/手機
        </th>
        <th>
            電子信箱
        </th>
        <th>
            生日
        </th>
        <th>
            會員註冊時間
        </th>
        <th>
            帳號
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.MbPassword)
        </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MbName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MbPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MbEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MbBrithday)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MbCreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MbAccount)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.MbPassword)
            </td>*@
            <td>
                @Html.ActionLink("修改我的資料", "Edit", new { id = item.MbIdentity }, new {@class= "btn btn-warning" }) 
               
               
            </td>
        </tr>
    }

</table>
